// c++ program to estimate value of pi

#include <iostream>

#include <cmath>	//includes math packacge

using namespace std;		//need for all programs


int main()
{
	int k;
	double val=0.0 , fval;

for (k=1;k<=10;k++)
{
val=val+ ( pow(-1,k+1)/(2*k-1) );
}

fval = 4*val;


printf( "Approximate PI=%14.12lf\n ", nval );
printf( "Built in PI=%14.12lf\n ", M_PI );   
return 0;
}
